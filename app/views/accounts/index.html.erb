<div class="container">
  <div class="row">
    <div class="col-8">
        <% @posts.each do |post| %>
          <div class="inst-post">
            <div class="post-meta">
              <%= post.account.username %>
            </div>
            <%= image_tag post.image %>
          </div>
        <% end %>
    </div>
  </div>
</div>
